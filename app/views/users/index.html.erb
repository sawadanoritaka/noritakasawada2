<% @users.each do |user| %>
<div class="row">
  <div class="col-xs-6 col-md-4">
   <div class="thumbnail">
    <%= profile_img(user) %>
    <div class="caption">
     <h3><%= user.name %></h3>
     <p><%= user.profile %></p>
     <p><a href="#" class="btn btn-primary" role="button">Button</a><a href="#" class="btn btn-default" role="button">Button</a></p>
    </div>
  </div>
</div>
<% end %>